//Selection sort

#include<stdio.h>
void main()
{
int a[100],i,j,s,n;
printf("Enter limit : ");
scanf("%d",&n);
printf("Enter the elements :  ");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
printf("Entered aray is : ");
for(i=0;i<n;i++)
{
printf("%d  ",a[i]);
}
for(i=0;i<(n-1);i++)
{
for(j=i;j<n-1;j++)
{
if(a[i]>a[j])
{
s=a[i];
a[i]=a[j];
a[j]=s;
}
}
}
printf("\nSorted array is : ");
for(i=0;i<n;i++)
{
printf("%d \n",a[i]);
}
printf("\n");
printf(" ");
}

